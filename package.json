{
  "name": "imcao_blog",
  "version": "1.0.0",
  "author": "reco_luan",
  "scripts": {
    "dev": "vuepress dev docs --host \"0.0.0.0\"",
    "build": "vuepress build docs",
    "lint": "prettier --write docs/**/*.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "git add . && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "husky": "1.3.1",
    "lint-staged": "7.2.0",
    "prettier": "^2.3.2",
    "vuepress": "1.7.1",
    "vuepress-theme-reco": "1.6.6"
  },
  "description": "ImCaO_Blog"
}
